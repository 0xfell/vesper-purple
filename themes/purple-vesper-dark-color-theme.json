  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#ed9cc2",
    "property": "#ed9cc2",
    "interface": "#ed9cc2",
    "type": "#ed9cc2",
    "class": "#ed9cc2"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {"foreground": "#ffffff4f"}
    },
    {
      "scope": ["delimiter.bracket", "delimiter", "invalid.illegal.character-not-allowed-here.html", "keyword.operator.rest", "keyword.operator.spread", "keyword.operator.type.annotation", "keyword.operator.relational", "keyword.operator.assignment", "keyword.operator.type", "meta.brace", "meta.tag.block.any.html", "meta.tag.inline.any.html", "meta.tag.structure.input.void.html", "meta.type.annotation", "meta.embedded.block.github-actions-expression", "storage.type.function.arrow", "meta.objectliteral.ts", "punctuation"],
      "settings": {"foreground": "#666666"}
    },
    {
      "scope": ["constant", "entity.name.constant", "variable.language", "meta.definition.variable"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": "variable.parameter.function",
      "settings": {"foreground": "#dbd7caee"}
    },
    {
      "scope": ["entity.name.tag", "tag.html"],
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": "entity.name.function",
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": ["keyword", "storage.type.class.jsdoc"],
      "settings": {"foreground": "#ffffff9f"}
    },
    {
      "scope": ["storage", "storage.type", "support.type.builtin", "constant.language.undefined", "constant.language.null"],
      "settings": {"foreground": "#ffffff9f"}
    },
    {
      "scope": ["text.html.derivative", "storage.modifier.package", "storage.modifier.import", "storage.type.java"],
      "settings": {"foreground": "#dbd7caee"}
    },
    {
      "scope": ["string", "string punctuation.section.embedded source", "attribute.value"],
      "settings": {"foreground": "#a3a3a3"}
    },
    {
      "scope": ["punctuation.definition.string", "punctuation.support.type.property-name"],
      "settings": {"foreground": "#a3a3a399"}
    },
    {
      "scope": "support",
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": ["property", "meta.property-name", "meta.object-literal.key", "entity.name.tag.yaml", "attribute.name"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["entity.other.attribute-name", "invalid.deprecated.entity.other.attribute-name.html"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["support.type.primitive", "entity.name.type"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": "namespace",
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["keyword.operator", "keyword.operator.assignment.compound", "meta.var.expr.ts"],
      "settings": {"foreground": "#ffffff9f"}
    },
    {
      "scope": "invalid.broken",
      "settings": {"fontStyle": "italic", "foreground": "#fca5a5"}
    },
    {
      "scope": "invalid.deprecated",
      "settings": {"fontStyle": "italic", "foreground": "#fca5a5"}
    },
    {
      "scope": "invalid.illegal",
      "settings": {"fontStyle": "italic", "foreground": "#fca5a5"}
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {"fontStyle": "italic", "foreground": "#fca5a5"}
    },
    {
      "scope": "carriage-return",
      "settings": {"fontStyle": "italic underline", "background": "#fca5a5", "foreground": "#0a0a0a", "content": "^M"}
    },
    {
      "scope": "message.error",
      "settings": {"foreground": "#fca5a5"}
    },
    {
      "scope": "string variable",
      "settings": {"foreground": "#a3a3a3"}
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {"foreground": "#eab308"}
    },
    {
      "scope": ["string.regexp.character-class", "string.regexp constant.character.escape", "string.regexp source.ruby.embedded", "string.regexp string.regexp.arbitrary-repitition"],
      "settings": {"foreground": "#a3a3a3"}
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {"foreground": "#eab308"}
    },
    {
      "scope": ["support.constant"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["keyword.operator.quantifier.regexp", "constant.numeric", "number"],
      "settings": {"foreground": "#eab308"}
    },
    {
      "scope": ["keyword.other.unit"],
      "settings": {"foreground": "#ffffff9f"}
    },
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": {"foreground": "#eab308"}
    },
    {
      "scope": "meta.module-reference",
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {"foreground": "#f59e0b"}
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {"fontStyle": "bold", "foreground": "#c391e6"}
    },
    {
      "scope": "markup.quote",
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": "markup.italic",
      "settings": {"fontStyle": "italic", "foreground": "#dbd7caee"}
    },
    {
      "scope": "markup.bold",
      "settings": {"fontStyle": "bold", "foreground": "#dbd7caee"}
    },
    {
      "scope": "markup.raw",
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": ["markup.deleted", "meta.diff.header.from-file", "punctuation.definition.deleted"],
      "settings": {"background": "#fca5a520", "foreground": "#fca5a5"}
    },
    {
      "scope": ["markup.inserted", "meta.diff.header.to-file", "punctuation.definition.inserted"],
      "settings": {"background": "#16a34a20", "foreground": "#16a34a"}
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {"background": "#f59e0b20", "foreground": "#f59e0b"}
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {"foreground": "#222222", "background": "#0369a1"}
    },
    {
      "scope": "meta.diff.range",
      "settings": {"foreground": "#c391e6", "fontStyle": "bold"}
    },
    {
      "scope": "meta.diff.header",
      "settings": {"foreground": "#0369a1"}
    },
    {
      "scope": "meta.separator",
      "settings": {"fontStyle": "bold", "foreground": "#0369a1"}
    },
    {
      "scope": "meta.output",
      "settings": {"foreground": "#0369a1"}
    },
    {
      "scope": ["brackethighlighter.tag", "brackethighlighter.curly", "brackethighlighter.round", "brackethighlighter.square", "brackethighlighter.angle", "brackethighlighter.quote"],
      "settings": {"foreground": "#dedcd590"}
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {"foreground": "#fca5a5"}
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link", "punctuation.definition.string.begin.markdown", "punctuation.definition.string.end.markdown"],
      "settings": {"foreground": "#a3a3a3"}
    },
    {
      "scope": ["markup.underline.link.markdown", "markup.underline.link.image.markdown"],
      "settings": {"foreground": "#dedcd590", "fontStyle": "underline"}
    },
    {
      "scope": ["type.identifier", "constant.other.character-class.regexp"],
      "settings": {"foreground": "#ed9cc2"}
    },
    {
      "scope": ["entity.other.attribute-name.html.vue"],
      "settings": {"foreground": "#c391e6"}
    },
    {
      "scope": ["invalid.illegal.unrecognized-tag.html"],
      "settings": {"fontStyle": "normal"}
    }
  ]
}
